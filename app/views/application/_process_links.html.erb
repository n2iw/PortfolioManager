  <div class='process-links list-inline'>
    <% if controller.action_name == 'show' %>
      <% if @process_picture_count > 0 %>
        <div class="process-link" id="process_link"><%= link_to button_tag('Process Pictures', type: 'button', class: 'btn btn-sm btn-primary'), action: :show_process %></div>
      <% end %>
    <% else %>
      <div class="process-link" id="finished_link"><%= link_to button_tag('Finished Pictures', type: 'button', class: 'btn btn-sm btn-primary'), action: :show %></div>
    <% end %>
  </div>

<% @title = @work.name %>
<div class="navigator">
  <div class="navigator_row">
    <div class="navigator_cell"><%= link_to 'Edit this work', edit_work_path %></div>
  </div>
</div>
<div class="content">
    <h1><%= @work.name %></h1>
    <p><%= @work.description %></p>
    <% @pictures.each do |p| %>
      <%= image_tag p.file.url(:large), alt: 'Picture' %>
    <% end %>
</div>

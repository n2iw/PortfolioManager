<div class="content" id="works">
  <table id="manage_works_table">
  <% @works.each do |work| %>
    <tr>
      <td><%= link_to image_tag(work.thumbnail.url(:thumb), alt: work.name), edit_work_path(work)  %></td>
      <td><%= form_for work, url: update_position_work_path(work) do |f| %> 
        <%= render partial: 'form', locals: {f: f} %>
          <% end %>
      </td>
    </tr>
  <% end %>
  </table>
</div>
